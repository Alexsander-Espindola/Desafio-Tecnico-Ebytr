{
  "root": true,
  "env": {
      "browser": true,
      "commonjs": true,
      "es2021": true
  },
  "extends": [
      "airbnb-base"
  ],
  "parserOptions": {
      "ecmaVersion": "latest"
  },
  "rules": {
      "no-console": "off",
      "no-unused-vars": ["error", { "vars": "next", "args": "after-used", "ignoreRestSiblings": true }]
  }
}
